:[[320,50]]}],"tablet":[{"code":"div-gpt-ad-728-top-0","bids":[{"bidder":"sovrn","sizes":[[728,90]],"params":{"tagid":"422090"}},{"bidder":"aol","sizes":[[728,90]],"params":{"placement":"4379193","network":"9924.1"}},{"bidder":"sonobi","sizes":[[728,90]],"params":{"ad_unit":"\/84077725\/CommunityTable\/food\/contrib\/alisonashton\/728_top"}},{"bidder":"gumgum","sizes":[[728,90]],"params":{"inSlot":"5385"}},{"bidder":"optimera","sizes":[[728,90]],"params":{"clientID":"13"}}],"sizes":[[728,90]]},{"code":"div-gpt-ad-728-bottom-1","bids":[{"bidder":"sovrn","sizes":[[728,90]],"params":{"tagid":"422090"}},{"bidder":"aol","sizes":[[728,90]],"params":{"placement":"4379193","network":"9924.1"}},{"bidder":"sonobi","sizes":[[728,90]],"params":{"ad_unit":"\/84077725\/CommunityTable\/food\/contrib\/alisonashton\/728_bottom"}},{"bidder":"gumgum","sizes":[[728,90]],"params":{"inSlot":"5386"}},{"bidder":"optimera","sizes":[[728,90]],"params":{"clientID":"13"}}],"sizes":[[728,90]]},{"code":"div-gpt-ad-300-top-2","bids":[{"bidder":"sonobi","sizes":[[300,600]],"params":{"ad_unit":"\/84077725\/CommunityTable\/food\/contrib\/alisonashton\/300_top"}},{"bidder":"sovrn","sizes":[[300,250]],"params":{"tagid":"422088"}},{"bidder":"aol","sizes":[[300,250]],"params":{"placement":"4379190","network":"9924.1"}},{"bidder":"gumgum","sizes":[[300,250]],"params":{"inSlot":"5380"}},{"bidder":"optimera","sizes":[[300,250],[300,600]],"params":{"clientID":"13"}}],"sizes":[[300,250],[300,600]]},{"code":"div-gpt-ad-300-bottom-3","bids":[{"bidder":"sonobi","sizes":[[300,250],[300,600]],"params":{"ad_unit":"\/84077725\/CommunityTable\/food\/contrib\/alisonashton\/300_bottom"}},{"bidder":"sovrn","sizes":[[300,250]],"params":{"tagid":"422088"}},{"bidder":"aol","sizes":[[300,250]],"params":{"placement":"4379190","network":"9924.1"}},{"bidder":"gumgum","sizes":[[300,250]],"params":{"inSlot":"5381"}},{"bidder":"optimera","sizes":[[300,250],[300,600]],"params":{"clientID":"13"}}],"sizes":[[300,250],[300,50],[300,600]]}]}},"adSlotId":"\/84077725\/CommunityTable\/food\/contrib\/alisonashton","firstLoadSlotTarget":false,"disableThirdPartyAds":false};

    (function (adSettings, app) {
        AdManager = {
            debugQueryString: "amgaddebug",
            debugOn: true,
            adSlotId: null,
            platform: 'desktop',
            adInstances: {},
            slots: [],
            slotSize: null,
            prebidEnable: false,
            prebidGranularity: 'auto',
            firstLoadChecked: false,
            firstLoadSlotId: 'div-gpt-first-load-slot',
            slotMarkerClass: 'ad-slot-marker',
            takeoverEnabled: false,
            wallpaperInfo: {
                'slotName' : 'wallpaper_background',
                'rendered' : false,
                'delayedRender' : false,
                'minWidth' : 1024,
                'slotDetails' : {
                    'outOfPage' : true,
                    'slotId' : 'div-gpt-wallpaper-background-99',
                    'localTargets' : {pos : "wallpaper", loc: "background"}
                },
                'fromFSET' : false,
                'hasBeenTracked' : false
            },
            immediateDisplaySlots: ['728_gallery', '300_gallery'],
            slotExclusionMap: {'wallpaper_background' : ['728_top']},
            blacklistedSlots: {},
            slotDisplayQueue: [],

            init: function () {
                this.setDebug();
                this.setEnvironment();
                this.setAdManagerParams(adSettings.managerParams);
                this.initGpt(this.platform, adSettings.globalTargets);
                this.manageWallpaperBlacklisting();

                if (this.shouldAttemptTakeover()) {
                    this.buildTakeoverSlot();
                } else {
                    this.buildSlots(adSettings.slots);
                }
                this.enableGpt();
            },
            setDebug: function() {
                this.debugOn = (window.location.search.toLowerCase().includes(this.debugQueryString));
            },
            setEnvironment: function () {
                if (typeof app !== 'undefined' && typeof app.platform !== 'undefined') {
                    this.platform = app.platform();
                }
            },
            setAdManagerParams: function (adManagerParams) {
                this.debug("Setting Ad Manager params", adManagerParams, true);
                for (var adManagerParam in adManagerParams) {
                    if (adManagerParams.hasOwnProperty(adManagerParam)) {
                        AdManager[adManagerParam] = adManagerParams[adManagerParam];
                    }
                }
            },
            initGpt: function(platform, globalTargets) {
                googletag.cmd.push(function() {
                    if (!globalTargets.platform) {
                        globalTargets.platform = platform;
                    }

                    this.addGlobalTargeting(globalTargets);

                    googletag.pubads().enableSingleRequest();
                    googletag.pubads().collapseEmptyDivs();
                }.bind(this));
            },
            enableGpt: function() {
                googletag.cmd.push(function() {
                    this.debugInfo("Enabling googletag");
                    googletag.enableServices();
                }.bind(this));
            },
            addGlobalTargeting: function(keyList) {
                this.debug("Setting Global Targets", keyList, true);

                for (var keyName in keyList) {
                    googletag.pubads().setTargeting(keyName, keyList[keyName]);
                }
            },
            buildSlots: function(slots) {
                // check to see if slots should exclude others.  This loop is done
                // ahead of the pushed function as blacklists need to be set first.
                for (var slotName in slots) {
                    this.manageSlotExclusions(slotName);
                }

                googletag.cmd.push(function() {
                    var slotDef;
                    for (var slotName in slots) {
                        if (!this.isBlacklisted(slotName)) {
                            slotDef = this.buildSlotDef(slots[slotName].defineSlot);

                            if (slotDef) {
                                this.adInstances[slotName] = slotDef;
                            } else {
                                this.debug("Skipping slot as there are no defined sizes: " + slotName);
                            }
                        }
                    }
                }.bind(this));
            },
            buildSlotDef: function (slotInfo) {
                this.debugInfo("Building slot with details", slotInfo);

                var slotDef, slotSizes;
                if (slotInfo.outOfPage) {
                    slotDef = googletag.defineOutOfPageSlot(this.adSlotId, slotInfo.slotId);
                } else {
                    slotSizes = this.getAdSizesBySlotName(slotInfo.slotName);
                    if (!slotSizes || slotSizes.length === 0) {
                        // no defined sizes for this ad, don't add slot.
                        return false;
                    }
                    slotDef = googletag.defineSlot(this.adSlotId, slotSizes, slotInfo.slotId);
                }

                slotDef.addService(googletag.pubads());

                if (slotInfo.hasOwnProperty("localTargets")) {
                    for (var slotTarget in slotInfo.localTargets) {
                        slotDef.setTargeting(slotTarget, slotInfo.localTargets[slotTarget]);
                    }
                }

                return slotDef;
            },
            displaySlot: function(slotName, slotId) {
                var execution = function() {
                    googletag.cmd.push(function() {
                        googletag.display(slotId);
                    });
                };
                execution.slotName = slotName;
                execution.slotId = slotId;

                if (this.readyToDisplayAds()) {
                    // not in first site entry takeover check, just display the thing.
                    this._actuallyDisplaySlot(execution);
                } else {
                    // we are in the middle of checking a site entry takeover,
                    // this slot is likely not defined yet, so don't display it.
                    this.slotDisplayQueue.push(execution);
                }
            },
            executeSlotDisplayQueue: function() {
                while (this.slotDisplayQueue.length > 0) {
                    this._actuallyDisplaySlot(this.slotDisplayQueue.shift());
                }

                //once the queue is empty, swap out the push method to prevent orphans
                this.slotDisplayQueue.push = this._actuallyDisplaySlot;
            },
            _actuallyDisplaySlot: function(execution) {
                // execution is a function object that will correctly call googletag.
                // It also has some information attached to it.
                if (this.isBlacklisted(execution.slotName)) {
                    return; // slot has been excluded, don't display
                }

                if (this.immediateDisplaySlots.indexOf(execution.slotName) !== -1) {
                    // this slot is in our "display immediately" list
                    execution();

                } else if (window.addEventListener) {
                    window.addEventListener('load', execution);

                } else if (window.attachEvent) {
                    window.attachEvent('onload', execution);
                }
            },
            moveSlotToMarker: function(slotMarker) {

                // slotMarker should be a dom element with a data-slotname attribute
                if (typeof slotMarker === "undefined"
                    || typeof slotMarker.dataset === "undefined"
                    || typeof slotMarker.dataset.slotname == "undefined"
                    || typeof slotMarker.appendChild !== "function"
                ) {
                    this.debugError("Attempting to move a slot to an invalid target", slotMarker);
                    return;
                }

                var slotName = slotMarker.dataset.slotname;
                var slotContainer;

                if (!adSettings.slots.hasOwnProperty(slotName) || !adSettings.slots[slotName].hasOwnProperty('slotId')) {
                    this.debugError("Attempting to move an unknown slot to marker", slotMarker);
                    return;
                }

                slotContainer = document.getElementById(adSettings.slots[slotName].slotId);

                if (slotContainer === null) {
                    this.debugError("Unable to find a rendered slot container for slot marker", slotMarker);
                    return;
                }

                if (slotContainer === slotMarker) {
                    this.debugInfo("Ad slot already contined in marker, skipping the move", slotMarker);
                    return;
                }

                // Make sure we can move it back to the old container if we need too
                if (!slotContainer.classList.contains(this.slotMarkerClass)) {
                    slotContainer.classList.add(this.slotMarkerClass);
                }

                if (typeof slotContainer.dataset.slotname === "undefined") {
                    slotContainer.dataset.slotname = slotName;
                }

                // Everything is good, move the slot over
                slotMarker.appendChild(slotContainer);
                this.debugInfo("Moved " + slotName + " to new dom node");
            },
            manageSlotExclusions: function (slotName) {
                if (this.slotExclusionMap.hasOwnProperty(slotName)) {
                    this.addBlacklistedSlots(this.slotExclusionMap[slotName])
                }
            },
            addBlacklistedSlots: function (slotNames) {
                for (var slotKey in slotNames) {
                    var slot = slotNames[slotKey];
                    if (!this.blacklistedSlots.hasOwnProperty(slot)) {
                        this.debugInfo("Blacklisting slot: " + slot);
                        this.blacklistedSlots[slot] = true;
                    }
                }
            },
            removeBlacklist: function (slotName) {
                this.debugInfo("Removing slot blacklist: " + slotName);
                delete this.blacklistedSlots[slotName];
            },
            isBlacklisted: function (slotName) {
                return this.blacklistedSlots.hasOwnProperty(slotName);
            },
            shouldAttemptTakeover: function () {
                return (this.firstLoadSlotTarget !== false);
            },
            readyToDisplayAds: function() {
                return (!this.shouldAttemptTakeover() || this.firstLoadChecked !== false);
            },
            buildTakeoverSlot: function() {
                googletag.cmd.push(function() {
                    this.debugInfo("Building takeover slot");

                    // adding a DOM element for the takeover
                    var takeoverContainer = document.createElement('div');
                    takeoverContainer.id = this.firstLoadSlotId;
                    document.body.appendChild(takeoverContainer)

                    var slotInfo = {
                        outOfPage: true,
                        slotId: this.firstLoadSlotId,
                        localTargets: {pos: this.firstLoadSlotTarget}
                     };

                    this.adInstances[this.firstLoadSlotId] = this.buildSlotDef(slotInfo);

                    googletag.display(takeoverContainer);
                    // add callback to check for non-responses
                    googletag.pubads().addEventListener('slotRenderEnded', this.handleTakeoverLoad.bind(this));
                }.bind(this));
            },
            buildWallpaperSlot: function() {
                googletag.cmd.push(function() {
                    var slotName = this.wallpaperInfo.slotName;
                    var slotId = this.wallpaperInfo.slotDetails.slotId;

                    if (this.adInstances.hasOwnProperty(slotName)) {
                        // wallpaper slot has already been built, skip this.
                        return;
                    }

                    this.debugInfo("Building wallpaper slot");

                    // make sure 728_top is excluded
                    this.manageSlotExclusions(slotName);

                    // adding a DOM element for the takeover
                    var container = document.createElement('div');
                    container.id = slotId;
                    document.body.appendChild(container)

                    this.adInstances[slotName] = this.buildSlotDef(this.wallpaperInfo.slotDetails);

                    this.displaySlot(slotName, slotId);
                }.bind(this));
            },
            enableTakeover: function(takeoverKey, hideWallpaper = false) {
                this.debugInfo("Site entry detected, enabling takeover: " + takeoverKey);

                this.takeoverEnabled = true;
                this.addGlobalTargeting({
                    'site_entry_takeover_set' : takeoverKey,
                    'takeover_group' : "takeover"
                });

                if (hideWallpaper) {
                    // Make sure that there is not a wallpaper slot created.
                    // This would be the case for a FSET + wallpaper
                    this.preventWallpaperSlot();
                } else if (!adSettings.slots.hasOwnProperty(this.wallpaperInfo.slotName)) {
                    // Wallpaper slot wasn't predefined, create this slot manually
                    this.buildWallpaperSlot();
                }
            },
            activateWallpaper: function(wallpaperDetails, renderMethod) {

                var wallpaperCallback = function() {
                    if (!this.checkShouldRenderWallpaper()) {
                        this.debugInfo("Skipping Wallpaper render as window is not wide enough (" + window.innerWidth
                            + "px < " + this.wallpaperInfo.minWidth + "px)");
                        return;
                    }
                    if (this.wallpaperInfo.rendered === true) {
                        this.debugInfo("Skipping wallpaper activation as it has already been rendered");
                        return;
                    }

                    this.debugInfo("Rendering wallpaper with info: ", wallpaperDetails);
                    this.wallpaperInfo.rendered = true;
                    this.trackLateWallpaperImpression();
                    window.parade.takeover = wallpaperDetails;
                };

                if (this.checkShouldRenderWallpaper()) {
                    this.debugInfo("Immediate wallpaper render detected");
                    wallpaperCallback.apply(this);
                } else {
                    this.debugInfo("Wallpaper not rendered as window is not wide enough, registering callback.");
                    this.wallpaperInfo.delayedRender = true;
                    window.addEventListener('resize', this.debounce(wallpaperCallback.bind(this), 150));
                }
            },
            manageWallpaperBlacklisting : function() {
                if (this.wallpaperInfo.rendered === true || this.wallpaperInfo.fromFSET === true) {
                    return;
                }

                // @todo set wallpaper breakpoint in ad admin
                if (!this.checkShouldRenderWallpaper()) {
                    this.debugInfo("Blacklisting wallpaper slot, width (" + window.innerWidth
                        + "px) smaller than breakpoint (" + this.wallpaperInfo.minWidth + "px)");
                    this.addBlacklistedSlots([this.wallpaperInfo.slotName]);
                    this.preventWallpaperSlot();
                    this.wallpaperInfo.delayedRender = true;
                    // listen for resize
                    window.addEventListener('resize', this.debounce(this.manageWallpaperBlacklisting.bind(this), 150));

                } else if (this.isBlacklisted(this.wallpaperInfo.slotName)) {
                    this.trackLateWallpaperImpression();
                    this.removeBlacklist(this.wallpaperInfo.slotName);
                    this.buildWallpaperSlot();
                    this.refreshAllAds();
                }
            },
            checkShouldRenderWallpaper: function() {
                // only render wallpaper if window it is bigger than the minwidth
                return (window.innerWidth >= this.wallpaperInfo.minWidth);
            },
            preventWallpaperSlot: function() {
                if (adSettings.slots.hasOwnProperty(this.wallpaperInfo.slotName)) {
                    delete adSettings.slots[this.wallpaperInfo.slotName];
                }
            },
            markWallpaperFromFSET: function() {
                // prevent leaderboard
                this.manageSlotExclusions(this.wallpaperInfo.slotName);
                this.wallpaperInfo.fromFSET = true;
            },
            wallpaperFromFSET: function() {
                return this.wallpaperInfo.fromFSET;
            },
            handleTakeoverLoad: function(event) {
                if (this.firstLoadChecked === true) {
                    // being careful, though this check shouldn't be needed as we are cleaning the slot
                    this.debugInfo("Bypassing first load check as it is already complete");
                    return;
                }

                if (!this.slotIsSiteEntryDetection(event.slot)) {
                    return;
                }

                this.debugInfo("Site Entry Slot returned", event);
                this.firstLoadChecked = true;

                if (event.isEmpty) {
                    this.debugInfo("Site entry slot indicates no takeover");
                }

                // clean up our first site entry slot
                googletag.destroySlots([this.adInstances[this.firstLoadSlotId]]);
                delete this.adInstances[this.firstLoadSlotId];

                // build all of our normal slots and then refresh
                this.buildSlots(adSettings.slots);
                this.refreshAllAds();
            },
            slotIsSiteEntryDetection: function(slot) {
                var mapping = slot.getTargetingMap();

                if (typeof mapping.pos == 'undefined' || mapping.pos.indexOf(this.firstLoadSlotTarget) === -1) {
                    // no position targeting on slot, or slot isn't targeted at first load detection.
                    this.debugInfo("FSE slot detection fail", mapping);
                    return false;
                }
                return true;
            },
            getAdSizesBySlotName: function (slotName) {
                if (typeof this.slots[slotName] !== 'undefined' && typeof this.slots[slotName][this.platform] !== undefined) {
                    return this.slots[slotName][this.platform];
                }

                return [];
            },
            getPrebidSlots: function () {
                if (typeof this.prebid !== 'undefined' && typeof this.prebid.slots !== 'undefined') {
                    return this.prebid.slots[this.platform];
                }

                return [];
            },
            getPrebidTimeout: function () {
                if (typeof this.prebid !== 'undefined' && typeof this.prebid.timeout !== 'undefined') {
                    return this.prebid.timeout;
                }

                return null;
            },
            getPrebidGranularity: function () {
                if (typeof this.prebid !== 'undefined' && typeof this.prebid.granularity !== 'undefined') {
                    return this.prebid.granularity;
                }

                return this.prebidGranularity;
            },
            refreshAllAds: function () {
                var refreshSlots = [];
                for (var adInstance in this.adInstances) {
                    if (this.adInstances.hasOwnProperty(adInstance)) {
                        refreshSlots.push(this.adInstances[adInstance]);
                    }
                }

                if (this.prebidEnable === true && typeof pbjs !== 'undefined') {
                    this.debugInfo("Refreshing all PBJS");
                    pbjs.que.push(function () {
                        pbjs.requestBids({
                            timeout: AdManager.getPrebidTimeout(),
                            bidsBackHandler: function () {
                                pbjs.setTargetingForGPTAsync();
                                googletag.pubads().refresh(refreshSlots);
                            }
                        });
                    });
                } else {
                    this.debugInfo("Refreshing all GPT");
                    googletag.pubads().refresh(refreshSlots);
                }
            },
            refreshAds: function (ads) {
                if (!ads) {
                    return false;
                }

                if (this.prebidEnable === true && typeof pbjs !== 'undefined') {
                    this.debugInfo("Prebid checking");
                    var slotIds = [];
                    for (var i = 0; i < ads.length; i++) {
                        if (ads[i]) {
                            slotIds.push(ads[i].getSlotElementId());
                        }
                    }

                    pbjs.que.push(function () {
                        pbjs.requestBids({
                            timeout: AdManager.getPrebidTimeout(),
                            adUnitCodes: slotIds,
                            bidsBackHandler: function () {
                                pbjs.setTargetingForGPTAsync(slotIds);
                                googletag.pubads().refresh(ads);
                            }
                        });
                    });
                } else {
                    this.debugInfo("No prebid checking");
                    googletag.pubads().refresh(ads);
                }
            },
            updateTargetingOnAllAdUnits: function (targetName, targetValue) {
                for (var key in this.adInstances) {
                    if (this.adInstances.hasOwnProperty(key)) {
                        this.adInstances[key].setTargeting(targetName, targetValue);
                    }
                }
            },
            getNumberOfDefinedAdUnits: function (ads) {
                var adCount = 0;
                if (!this.readyToDisplayAds()) {
                    this.debugInfo("Not ready to count ad instances, delaying");
                    return false;
                }


                if (!ads) {
                    ads = this.adInstances;

                    if (this.wallpaperInfo.rendered === true && this.wallpaperFromFSET() && !this.wallpaperHasBeenTracked()) {
                        // manually ad wallpaper that came in from FSET.
                        adCount++;
                        this.wallpaperHasBeenTracked(true);
                    }
                }

                if (!(ads instanceof Array) && !(ads instanceof Object)) {
                    // can't iterate over it
                    this.debugWarn("Unable to count supplied ad instances", ads);
                    return adCount;
                }

                for (var adInstance in ads) {
                    if (isFinite(adInstance) && this.isSlotObject(ads[adInstance])) {
                        // this is a googletag slot object and is always counted
                        adCount++;

                    } else if (this.shouldTrack(adInstance)) {
                        adCount++;
                    }
                }

                this.debugInfo("Reporting " + adCount + " ad instances for tracking");
                return adCount;
            },
            isSlotObject: function(subject) {
                // This is a potentially fragile implementation. If DFP changes their object api,
                // the duck typing may fail.  The method names in the array were chosen as they
                // seemed unlikely to all exist on a non-slot object or to be removed/changed
                // in DFP updates.  They must all exist and be functions for the check to pass.
                var requiredMethods = ['getSlotId', 'getSlotElementId', 'getOutOfPage'];

                for (var checkIndex = 0; checkIndex < requiredMethods.length; checkIndex++) {
                    if (typeof subject[requiredMethods[checkIndex]] !== 'function') {
                        // the subject object doesn't have
                        return false;
                    }
                }
                // subject has all of the methods and is likely a googletag slot.
                return true;
            },
            shouldTrack: function(slotName) {
                if (!this.adInstances.hasOwnProperty(slotName) || this.isBlacklisted(slotName)) {
                    // unknown or blacklisted slot.
                    this.debugInfo("Not tracking slot: " + slotName);
                    return false;
                }

                if (slotName == this.wallpaperInfo.slotName) {
                    // this is the wallpaper slot
                    if (this.wallpaperHasBeenTracked()) {
                        // we've already tracked it.
                        this.debugInfo("Not tracking wallpaper slot as it has already been noted");
                        return false;
                    }

                    // mark it so we don't track it again then return
                    this.wallpaperHasBeenTracked(true);
                    return true;
                }

                return true;
            },
            wallpaperHasBeenTracked: function(status) {
                if (typeof status !== "undefined") {
                    this.wallpaperInfo.hasBeenTracked = (status !== false);
                    this.debugInfo("Wallpaper tracking marked as", this.wallpaperInfo.hasBeenTracked);
                }

                return this.wallpaperInfo.hasBeenTracked;
            },
            shouldDisplay3rdPartyAds: function () {
                if (!this.readyToDisplayAds()) {
                    return null;
                }

                // don't show 3rd party ads when manually disabled in admin or takeover active
                if (app.disableThirdPartyAds === true
                    || adSettings.disableThirdPartyAds === true
                    || this.takeoverEnabled
                    || this.hasAd(this.wallpaperInfo.slotName)
                ) {
                    return false;
                }
                // nothing blocking it, lets display!
                return true;
            },
            hasAd: function (adName) {
                var hasAd = false;
                if (this.adInstances[adName]) {
                    hasAd = true;
                }

                return hasAd;
            },
            displayOrientationChangeAds: function () {
                var tabletRefreshableSizes = [];
                for (var adInstance in this.adInstances) {
                    if (this.adInstances.hasOwnProperty(adInstance) && (adInstance === '300_top' || adInstance === '300_bottom')) {
                        tabletRefreshableSizes.push(this.adInstances[adInstance]);
                    }
                }

                this.refreshAds(tabletRefreshableSizes);
                this.trackAdDisplays(tabletRefreshableSizes);
            },
            trackAdDisplays: function (ads) {
                var adCount = this.getNumberOfDefinedAdUnits(ads);
                if (adCount === false) {
                    return;
                }
                ga('amgTracker.send', 'event', 'displayed-ads', 'total', window.document.location.pathname, adCount, {'nonInteraction': true});
            },
            trackLateWallpaperImpression: function() {
                if (this.wallpaperInfo.delayedRender !== true) {
                    return;
                }
                ga('amgTracker.send', 'event', 'displayed-ads', 'total', window.document.location.pathname, 1, {'nonInteraction': true});
                app.contribReporter.sendContributorEvent(1, document.location.pathname);
            },
            listenSingle: function(target, eventName, eventCallback) {
                target.addEventListener(eventName, function(e) {
                    // remove the event
                    e.target.removeEventListener(e.type, arguments.callee);

                    // call the actual event handler.
                    eventCallback(e);
                });
            },
            // Debounce from underscore.js
            debounce: function(func, wait, immediate) {
            	var timeout;
            	return function() {
            		var context = this;
                    var args = arguments;

            		var later = function() {
            			timeout = null;
            			if (!immediate) {
                            func.apply(context, args);
                        }
            		};

            		var callNow = immediate && !timeout;
            		clearTimeout(timeout);

            		timeout = setTimeout(later, wait);
            		if (callNow){
                         func.apply(context, args);
                     }
            	};
            },
            debug: function (label, value, asTable) {
                if (this.debugOn !== true) {
                    return;
                }

                console.log("AMG_ADS -- " + label);

                if (typeof value === "undefined") {
                    return;
                }

                (asTable == true) ? console.table(value) : console.log(value);
            },
            debugInfo: function(label, value) {
                if (this.debugOn !== true) {
                    return;
                }
                console.info("AMG_ADS -- " + label);

                if (typeof value !== "undefined") {
                    console.info(value);
                }
            },
            debugWarn: function(label, value) {
                if (this.debugOn !== true) {
                    return;
                }
                console.warn("AMG_ADS -- " + label);

                if (typeof value !== "undefined") {
                    console.warn(value);
                }
            },
            debugError: function(label, value) {
                if (this.debugOn !== true) {
                    return;
                }
                console.error("AMG_ADS -- " + label);

                if (typeof value !== "undefined") {
                    console.error(value);
                }
            }
        };

        AdManager.init();
    })( window.parade.adSettings, window.parade);

    // @todo: cleanup this is legacy code used to detect if function calls are being made that shouldn't be at this point.
    window.parade = (function (app, AdManager) {
        app.trackAdDisplays = function (ads) {
            console.log('legacy track ad displays ads call............');
        };

        app.displayInitialAds = function () {
            console.log('legacy display initial ads call............');
        };

        app.orientationChanged = false;
        app.displayOrientationChangeAds = function () {
            console.log('legacy display orientation change call............');
            if (typeof app.is_tablet !== 'undefined' && (app.is_tablet === true && app.is_landscape === false)) {
                AdManager.displayOrientationChangeAds();
            }
        };

        app.adsReady = true;
        app.refreshAds = function (ads) {
            console.log('legacy refresh ads call............');
        };

        return app;
    })(window.parade || {}, AdManager);
</script>
<script type="text/javascript" src="//static-communitytable.parade.com/wp-content/plugins/amg_ads/assets/js/lib/Prebid/build/dist/prebid.js" async></script>
<script>
    var PREBID_TIMEOUT = 500;
    var adUnits = [];
    if (typeof window.AdManager !== 'undefined') {
        PREBID_TIMEOUT = window.AdManager.getPrebidTimeout();
        adUnits = window.AdManager.getPrebidSlots();
    }

    function initializePrebid() {
        pbjs.que.push(function () {
            pbjs.addAdUnits(adUnits);
            pbjs.requestBids({
                bidsBackHandler: sendAdserverRequest
            });
        });

        function sendAdserverRequest() {
            if (pbjs.adserverRequestSent) return;
            pbjs.adserverRequestSent = true;
            googletag.cmd.push(function () {
                pbjs.que.push(function () {
                    pbjs.setTargetingForGPTAsync();
                    googletag.pubads().refresh();
                });
            });
        }

        setTimeout(function () {
            sendAdserverRequest();
        }, PREBID_TIMEOUT);
    }

    if (window.addEventListener) {
        window.addEventListener('load', initializePrebid);
    } else if (window.attachEvent) {
        window.attachEvent('onload', initializePrebid);
    }

</script>
<script>
    (function () {
        var gads = document.createElement('script');
        gads.async = true;
        gads.type = 'text/javascript';
        var useSSL = 'https:' == document.location.protocol;
        gads.src = (useSSL ? 'https:' : 'http:') +
            '//www.googletagservices.com/tag/js/gpt.js';
        var node = document.getElementsByTagName('script')[0];
        node.parentNode.insertBefore(gads, node);
    })();
